        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "echo \"No tests yet\"",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "consola": "^3.4.0",
    "dotenv": "^17.2.2",
    "glob": "^11.0.1"
  }
}
