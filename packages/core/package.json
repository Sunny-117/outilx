{
  "name": "@outilx/core",
  "version": "1.0.0",
  "description": "Core JavaScript utility functions for any runtime (Node.js, Deno, Bun, browsers)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest --run",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "fast-json-stable-stringify": "^2.1.0"
  }
}
